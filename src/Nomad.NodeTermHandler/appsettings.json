{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.DataProtection": "None"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {}
      }
    ]
  },

  "AllowedHosts": "*"
}
